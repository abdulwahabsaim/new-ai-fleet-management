{
  "name": "ai-fleet-pro-node-app",
  "version": "1.0.0",
  "description": "Node.js web app for the AI Fleet Management System.",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev:node": "nodemon server.js",
    "dev:py": "cd ../python-service && source venv/bin/activate && flask run --port 5001",
    "dev": "concurrently \"npm:dev:node\" \"npm:dev:py\"",
    "seed": "node ./seeders/seed.js"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "connect-flash": "^0.1.1",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "method-override": "^3.0.0",
    "mongoose": "^8.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.23.6",
    "@babel/node": "^7.22.19",
    "@babel/preset-env": "^7.23.6",
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2"
  }
}